void update(int *arr,int size,int num){
    for(int i = 0; i<size;i++){
        *(arr+i) = *(arr+i)+10;
    }
}

int main(){
    int arr [] = {1,2,3,4};
    int size = sizeof(arr)/sizeof(arr[0]);
    int num = 10;
    update(arr,size,num);

    for(int i = 0;i<size;i++){
        cout<<arr[i]<<endl;
    }
    
}
